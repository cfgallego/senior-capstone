
@{
    ViewBag.Title = "Next";
}

<h3>Send Notification</h3>

<form class="nextID" action="#" method="post">
    <label>Emergency:</label>
    <input type="text" id="emergency" name="" value="">

    <label>Date:</label>
    <input type="text" id="date" name="" value="">

    <label>Time:</label>
    <input type="text" id="time" name="" value="">

    <label>Where:</label>
    <input type="text" name="emergencyAddress">

    <label>Additional Comments:</label>
    <textarea name="name" rows="4" cols="40"></textarea>

    <input type="submit">
</form>